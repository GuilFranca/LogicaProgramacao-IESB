programa {
  funcao inicio() {
    // 1 - Escreva um algoritmo que leia dois números informados pelo usuário e exiba a soma entre eles.

    real numero1, numero2

    escreva("Digite o primeiro número: ")
    leia(numero1)

    escreva("Digite o segundo número: ")
    leia(numero2)

    escreva("A soma dos números é ", numero1 + numero2)

  }
}
